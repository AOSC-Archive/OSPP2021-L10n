#: job.cpp:61
#, kde-format
msgid "Service not available"
msgstr "服务不可用"

#: job.cpp:63
#, kde-format
msgid "Mailbox unavailable. The server said: %1"
msgstr "无法访问此邮箱，服务器响应为：%1"

#: job.cpp:65
#, kde-format
msgid "Insufficient storage space on server. The server said: %1"
msgstr "服务器的存储空间不足，服务器响应为：%1"

#: job.cpp:67
#, kde-format
msgid "Server error: %1"
msgstr "服务器出错：%1"

#: job.cpp:76
#, kde-format
msgid "Connection to server lost."
msgstr "与服务器的连接丢失。"

#: loginjob.cpp:81
#, kde-format
msgid "Login"
msgstr "登录时"

#: loginjob.cpp:146
#, kde-format
msgid "STARTTLS is not supported by the server, try using SSL/TLS instead."
msgstr "服务器不支持 STARTTLS，请尝试使用 SSL/TLS 代替。"

#: loginjob.cpp:209
#, kde-format
msgid "Could not authenticate to the SMTP server because no matching authentication method has been found"
msgstr "无法认证 SMTP 服务器因为没有匹配的认证方式"

#: loginjob.cpp:270
#, kde-format
msgid "Token expired"
msgstr "令牌已失效"

#: loginjob.cpp:314
#, kde-format
msgid "Login failed, cannot initialize the SASL library"
msgstr "登录失败，无法初始化 SASL 库"

#: sendjob.cpp:53
#, kde-format
msgid "SendJob"
msgstr "发送任务"

#: sendjob.cpp:104
#, kde-format
msgid "Could not send the message because either the sender or recipient field is missing or invalid"
msgstr "无法发送信件因为发信人或者收件人缺失或无效"

#: sendjob.cpp:112
#, kde-format
msgid "Could not send the message because it exceeds the maximum allowed size of %1 bytes. (Message size: %2 bytes.)"
msgstr "无法发送信件因为它超过了最大允许的 %1 字节大小。(信件大小：%2 字节。)"

