#: alarmdialog.cpp:144
#, kde-format
msgctxt "@title:window"
msgid "Reminders"
msgstr "提醒"

#: alarmdialog.cpp:165
#, kde-format
msgctxt "@action:button"
msgid "Dismiss Reminder"
msgstr "排除提醒"

#: alarmdialog.cpp:166
#, kde-format
msgctxt "@info:tooltip"
msgid "Dismiss the reminders for the selected incidences"
msgstr "对所选事件排除提醒"

#: alarmdialog.cpp:168
#, kde-format
msgctxt "@info:whatsthis"
msgid "Press this button to dismiss the currently selected incidence.  All non-selected non-incidences will be unaffected."
msgstr "点击此按钮可停止已选中的事件，所有未选中的非事件项目都不会受任何影响。"

#: alarmdialog.cpp:171
#, kde-format
msgctxt "@action:button"
msgid "Dismiss All"
msgstr "全部排除"

#: alarmdialog.cpp:172
#, kde-format
msgctxt "@info:tooltip"
msgid "Dismiss the reminders for all listed incidences"
msgstr "对所有列出的事件排除提醒"

#: alarmdialog.cpp:173
#, kde-format
msgctxt "@info:whatsthis"
msgid "Press this button to dismiss all the listed incidences."
msgstr "点击此按钮可停止下列的全部事件。"

#: alarmdialog.cpp:174
#, kde-format
msgctxt "@action:button"
msgid "Edit..."
msgstr "编辑..."

#: alarmdialog.cpp:175
#, kde-format
msgctxt "@info:tooltip"
msgid "Edit the selected incidence"
msgstr "编辑所选事件"

#: alarmdialog.cpp:177
#, kde-format
msgctxt "@info::whatsthis"
msgid "Press this button if you want to edit the selected incidence.  A dialog will popup allowing you to edit the incidence."
msgstr "点击此按钮将弹出一个对话框并可编辑所选的事件。"

#: alarmdialog.cpp:179
#, kde-format
msgctxt "@action:button"
msgid "Suspend"
msgstr "推迟"

#: alarmdialog.cpp:181
#, kde-format
msgctxt "@info:tooltip"
msgid "Suspend the reminders for the selected incidences by the specified interval"
msgstr "中止所选的按特定时间间隔发出提醒的事件"

#: alarmdialog.cpp:184
#, kde-format
msgctxt "@info:whatsthis"
msgid "Press this button to suspend the currently selected incidences.  The suspend interval is configurable by the Suspend duration settings."
msgstr "点击此按钮可暂停当前所选的事件，您可在“暂停时长”的设置中配置暂停间隔。"

#: alarmdialog.cpp:197
#, kde-format
msgctxt "@label"
msgid "Reminders: Clicking on the title toggles details for item"
msgstr "提醒：点击标题切换各项目的详情"

#: alarmdialog.cpp:205
#, kde-format
msgctxt "@title:column reminder summary"
msgid "Summary"
msgstr "主题"

#: alarmdialog.cpp:206
#, kde-format
msgctxt "@title:column happens at date/time"
msgid "Date Time"
msgstr "日期和时间"

#: alarmdialog.cpp:206
#, kde-format
msgctxt "@title:column trigger date/time"
msgid "Trigger Time"
msgstr "触发时间"

#: alarmdialog.cpp:211
#, kde-format
msgctxt "@info:tooltip"
msgid "The event or to-do summary"
msgstr "日程或待办事项的主题"

#: alarmdialog.cpp:214
#, kde-format
msgctxt "@info:tooltip"
msgid "The reminder is set for this date/time"
msgstr "已为此日期/时间设定提醒"

#: alarmdialog.cpp:218
#, kde-format
msgctxt "@info:tooltip"
msgid "The date/time the reminder was triggered"
msgstr "此日期/时间上的提醒已被触发"

#: alarmdialog.cpp:233
#, kde-kuit-format
msgctxt "@info default incidence details string"
msgid "<emphasis>Select an event or to-do from the list above to view its details here.</emphasis>"
msgstr "<emphasis>请在上方列表选择一个事件、待办事项查看其细节。</emphasis>"

#: alarmdialog.cpp:245
#, kde-format
msgctxt "@label:spinbox"
msgid "Suspend &duration:"
msgstr "中止间隔(&D)："

#: alarmdialog.cpp:252
#, kde-format
msgctxt "@info:tooltip"
msgid "Suspend the reminders by this amount of time"
msgstr "中止所选的在这段时间内会发出提醒的事件"

#: alarmdialog.cpp:254
#, kde-format
msgctxt "@info:whatsthis"
msgid "Each reminder for the selected incidences will be suspended by this number of time units. You can choose the time units (typically minutes) in the adjacent selector."
msgstr "每个所选事项都会被推迟过指定时间。您可以在旁边的选择器中选择时间单位(一般是分钟)。"

#: alarmdialog.cpp:262
#, kde-format
msgctxt "@item:inlistbox suspend in terms of minutes"
msgid "minute(s)"
msgstr "分钟"

#: alarmdialog.cpp:263
#, kde-format
msgctxt "@item:inlistbox suspend in terms of hours"
msgid "hour(s)"
msgstr "小时"

#: alarmdialog.cpp:264
#, kde-format
msgctxt "@item:inlistbox suspend in terms of days"
msgid "day(s)"
msgstr "天"

#: alarmdialog.cpp:265
#, kde-format
msgctxt "@item:inlistbox suspend in terms of weeks"
msgid "week(s)"
msgstr "周"

#: alarmdialog.cpp:266
#, kde-format
msgctxt "@info:tooltip"
msgid "Suspend the reminders using this time unit"
msgstr "中止使用此时间单位的提醒事件"

#: alarmdialog.cpp:268
#, kde-format
msgctxt "@info:whatsthis"
msgid "Each reminder for the selected incidences will be suspended using this time unit. You can set the number of time units in the adjacent number entry input."
msgstr "每个所选事项都会被中止指定时间。您可以在旁边的选择器中选择时间单位(一般是分钟)。"

#: alarmdialog.cpp:457
#, kde-format
msgctxt "@info"
msgid "\"%1\" is a read-only incidence so modifications are not possible."
msgstr "“%1”为只读事件，故无法进行修改。"

#: alarmdialog.cpp:463
#, kde-format
msgctxt "@info"
msgid "An internal error occurred attempting to modify \"%1\". Unsupported type."
msgstr "尝试修改“%1”时发生内部错误，类型不支持。"

#: alarmdialog.cpp:684
#, kde-format
msgctxt "@title"
msgid "Reminder"
msgstr "提醒"

#: alarmdialog.cpp:686 alarmdialog.cpp:689
#, kde-format
msgctxt "@title"
msgid "Reminder: %1"
msgstr "提醒：%1"

#: alarmdialog.cpp:705
#, kde-format
msgctxt "@info email subject, error message"
msgid "<warning>Failed to send the Email reminder for %1. %2</warning>"
msgstr "<warning>无法发送 %1 的邮件提醒。%2</warning>"

#: alarmdialog.cpp:1000
#, kde-format
msgctxt "@info"
msgid "Could not start KOrganizer so editing is not possible."
msgstr "无法启动 KOrganizer，不能进行编辑。"

#: alarmdialog.cpp:1008
#, kde-format
msgctxt "@info"
msgid "An internal KOrganizer error occurred attempting to modify \"%1\""
msgstr "尝试修改“%1”时发生 KOrganizer 内部错误"

